{
    "builds": [{
        "src": "agencyapp/wsgi.py",
        "use":"@vercel/python",
        "config":{"maxLamdaSize": "15mb", "runtime":"python3.9" }
    }],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"agencyapp/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        }
    ]
}